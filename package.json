{
  "name": "MarkuPrev",
  "version": "v1.3.1",
  "description": "markup previewer",
  "author": {
    "name": "Satoshi SAKAO"
  },
  "license": "MIT",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "start": "electron out/main/index.js",
    "lint": "eslint",
    "electron:build:win": "yarn build && electron-builder --win --config",
    "electron:build:mac": "yarn build && electron-builder --mac --config",
    "electron:build:linux": "yarn build && electron-builder --linux --config",
    "check-license": "license-checker --production --onlyAllow \"MIT;BSD-3-Clause;BSD-2-Clause;0BSD;ISC;Apache-2.0;Python-2.0;CC-BY-4.0;BlueOak-1.0.0\" --unknown --summary --excludePackages=\"@vue/compiler-sfc@2.7.16\"",
    "clean": "rm -rf out dist"
  },
  "main": "out/main/index.js",
  "dependencies": {
    "asciidoctor": "^3.0.4",
    "datauri": "^4.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-checkbox": "^1.1.0",
    "minimist": "^1.2.8",
    "node-html-parser": "^7.0.1",
    "sass": "^1.93.3",
    "vue": "^3.5.22",
    "vuetify": "^3.10.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron/notarize": "^3.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^4.0.1",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.5.1",
    "license-checker": "^25.0.1",
    "node-watch": "^0.7.4",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.2",
    "vite": "^7.1.12",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-eslint-parser": "^10.2.0",
    "vue-template-compiler": "^2.7.13"
  }
}
